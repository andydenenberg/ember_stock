<form class="form-horizontal">
  <legend>Delete Stock</legend>

<h2 class='offset1'>{{symbol}}</h2>
<dl class="dl-horizontal">


    <dt>Quantity:</dt>
    <dd>{{quantity}}</dd>
</dl>

<div class="form-actions">
<a class="btn btn-mini btn-danger" {{action destroyRecord}}>Delete</a>
  <button class="btn btn-mini" {{action cancel}}>Cancel</button>
</div>

</form>

